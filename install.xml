<!--
Installation model

    1. by path & name
    path - absolute path to the file
    name - file name

    2. type
    * systemd-tmpfiles  - install $(project.name) to /usr/lib/systemd/tmpfiles.d/
     -->

<install>
    <item type = "systemd-tmpfiles" />
    <item path = "/var/lib/fty/fty-alert-engine" name = "warranty.rule" />
    <item path = "/usr/share/bios/fty-autoconfig" />
</install>
